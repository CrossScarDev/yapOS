{
  "telemetry.enable": false,
  "runtime.version": "Lua 5.4",
  "runtime.special": {
    "import": "require"
  },
  "runtime.nonstandardSymbol": [
    "+=",
    "-=",
    "*=",
    "/="
  ],
  "diagnostics.globals": [
    "playdate",
    "json"
  ],
  "diagnostics.libraryFiles": "Disable",
  "completion.callSnippet": "Replace",
  "workspace.library": [
    "$PLAYDATE_SDK_PATH/CoreLibs"
  ]
}
